{% extends "::index.html.twig" %}

{% block title %}JEWEL STREET:SELLER LIST{% endblock %}
{% block jsscript %}
    {% stylesheets 'bundles/goldtaskapp/css/dataTables.bootstrap.css' filter="cssrewrite" %}			
    <link rel="stylesheet" href="{{ asset_url }}" type="text/css" media="all">
    {% endstylesheets %}
    {% javascripts 'bundles/goldtaskapp/js/jquery.dataTables.js' %}	
    <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}
    {% javascripts 'bundles/goldtaskapp/js/dataTables.bootstrap.js' %}	
    <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}
    <script type="text/javascript">
        $(function() {
            $('#example1').dataTable();
        });
    </script>
{% endblock %}
{% block bodyheader %}
    <section class="content-header">
        <h1>Users</h1>
        <ol class="breadcrumb">
            <li><a href="{{ path('goldtaskReportPatients') }}"><i class="fa fa-dashboard"></i> Home</a></li>			
            <li class="active">Sellers</li>
        </ol>
    </section>
    <!-- Main content -->
    <section class="content">
        <div class="row">
            <div class="col-xs-12">
                <div class="box">
                    <div class="box-header">
                        <h3 class="box-title">Welcome to Jewel Street</h3>	
						<input type='hidden' name='adminRedirection' value={{ adminRedirection }} id='adminRedirection'>
                        <input type='hidden' name='user_role_id' value={{ user_role_id }} id='user_role_id'>
						<input type='hidden' name='user_id' value={{ userid }} id='user_id'>
                        <input type='hidden' name='currentRoute' value={{ currentRoute }} id='currentRoute'>                        					
                        <div class="muted pull-left" style="float:right !important;padding-top:10px !important;padding-right:10px !important;"><a class="btn btn-primary" href="{{ path('goldtaskassignUser') }}" style="color:white !important;"><i class="fa fa-plus"></i>Clock In</a></div>
                    </div>
					<!-- /.box-header -->
                    <div class="box-body">
                        <table id="example1" class="table table-bordered table-striped">
                            
                            <tbody>	
                                Please Clock In --- 								
                            </tbody>
                        </table>
                    </div><!-- /.box-body -->
                </div><!-- /.box -->
            </div>
        </div>
    </section><!-- /.content -->   
{% endblock %}
